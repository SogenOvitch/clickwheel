<Application x:Class="Clickwheel.DeviceHelper.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Clickwheel.DeviceHelper.GUI"
             xmlns:wpfui="http://schemas.lepo.co/wpfui/2022/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <wpfui:ThemesDictionary Theme="Dark" />
                <wpfui:ControlsDictionary />
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="Icon" UriSource="icon.png" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
